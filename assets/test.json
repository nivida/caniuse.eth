{
    "tests": [
        {
            "_comment": "Tests a balance transfer between two addresses",
            "method": "eth_getBalance",
            "params": ["0xfce1C2b5D6aB2C6cfE75f9D3581Cb55FBaF57968"],
            "expected": "0x63",
            "stateChange": [
                {
                    "type": "transfer",
                    "from": "0xfce1C2b5D6aB2C6cfE75f9D3581Cb55FBaF57968",
                    "to": "0x3c3BBab6b0A5BAe8a75edf4c623985F09eC91c62",
                    "amount": "0x1"
                }
            ]
        },
        {
            "_comment": "Tests the balance of the receiver from the previous test",
            "method": "eth_getBalance",
            "params": ["0x3c3BBab6b0A5BAe8a75edf4c623985F09eC91c62"],
            "expected": "0x1"
        },
        {
            "_comment": "Tests the nonce of the sender from the previous transaction",
            "method": "eth_getTransactionCount",
            "params": ["0xfce1C2b5D6aB2C6cfE75f9D3581Cb55FBaF57968"],
            "expected": "0x1"
        },
        {
            "_comment": "Tests the nonce of the receiver from the previous transaction",
            "method": "eth_getTransactionCount",
            "params": ["0x3c3BBab6b0A5BAe8a75edf4c623985F09eC91c62"],
            "expected": "0x0"
        },
        {
            "_comment": "Tests the nonce of the receiver from the previous transaction",
            "method": "eth_blockNumber",
            "params": [],
            "expected": ""
        }
    ]
}